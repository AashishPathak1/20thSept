<%- include('./base/header.ejs') %>
    <div class="container">
        <div class="row">
            <h2 class="text-center my-3" style="color: rgb(19, 134, 3);"> <strong>The Student List is here!!!</strong>
            </h2>
            <table class="table text-center table-dark">
                <thead>
                    <tr>
                        <th>Roll No.</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Father Name</th>
                        <th>Aadhar-Card Number</th>
                        <th>Mobile Number</th>
                    </tr>
                </thead>
                <tbody>
                    <% students.forEach((student)=> { %>
                        <tr>
                            <td>
                                <%= student.rollNo %>
                            </td>
                            <td>
                                <%= student.firstName %>
                            </td>
                            <td>
                                <%= student.lastName %>
                            </td>
                            <td>
                                <%= student.fatherName %>
                            </td>
                            <td>
                                <%= student.aadharCardNo %>
                            </td>
                            <td>
                                <%= student.mobileNo %>
                            </td>
                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </div>

    </div>
    <%- include('./base/footer.ejs') %>